<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <% var i;%>
    <% var j;%>
    <% var arr=["January","February","March","April","May","June","July","August","September","October","November","December"];%>
    <% for(i=1;i<=12;i++){%>
         <% for(j=1;j<=31;j++){%>
           <div class="date" id= <%-arr[i-1]%><%-j%>><b><%-j%> <%-arr[i-1]%></b> </div>
           <%}%>
      <%}%>
  </body>
</html>
